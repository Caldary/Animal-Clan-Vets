<h1>Vets</h1>
<p><li><a href="/vets/new">Register New Vet</a></li></p>

<% for vet in @vets %>
    <p>Name: <%= vet.name %></p>
    <p>Age: <%= vet.age %></p>
    <p>Qualifications: <%= vet.qualifications %></p>
    <p><%= vet.id %></p>
    <p><li><a href="/vets/<%= vet.id %>">Details</a></li></p>
    <form action="/vets/<%= vet.id %>/delete" method="POST">
        <input class="btn btn--danger" type="submit" value="delete" />
    </form>
    <hr/>
<% end %>